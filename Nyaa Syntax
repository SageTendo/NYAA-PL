<PROGRAM> ::= "nyaa" <VARIABLE> {}

<FUNCTION> ::= "nyaa_func" <VARIABLE> "(" [<TYPE> <VARIABLE> {, <TYPE> <VARIABLE>}] ")" "{" <BODY> "}"

*** <BODY> ::= ""



<IF> ::= "if" "(" <EXPR> ")" "{" <STATEMENTS> {"elif" "(" <EXPR> ")" "{" <STATEMENTS> "}"} ["else" "{" <STATMENTS> "}"]

<WHILE> ::= "while" "(" <EXPR> ")" "{" <STATEMENTS> "}" ["else" "{" <STATEMENTS> "}"]

<FOR> ::= "for" "(" <ID> "->" <NUMBER> [":" <NUMBER>] ")" "{" <STATMENTS> "}"

*** <VAR_DEF> ::= <ID> "::" <TYPE>

<ID> ::= <LETTER> {<LETTER> | <DIGIT>}

<STATEMENTS> ::= "pass" | "continue" | <STATEMENT> {<STATEMENT>}

*** <STATEMENT> ::=

*** <SAY> ::= "say" "(" (<STRING> | <EXPR>) ")"

<ADD_OP> ::= "+" | "-" | "||"

<MUL_OP> ::= "*" | "&&" | "/" | "%"

<TYPE> ::= ("int"|"str") ["[" "]"]

<STRING> ::= '"' {(printable ascii)} '"'

<NUMBER> ::= <DIGIT> {<DIGIT>}

<LETTER> ::= "a" | ... | "z" | "A" | ... | "Z" | "_"

<DIGIT> ::= "0" | ... | "9"

